{% extends 'painless/base.painless'%}
{% block content %}

{{col}} = {{col}}.toString();
if ({{col}} instanceof String) {
    if ({{col}} ==~ /\d{4}-(0?[1-9]|1[0-2])-(0?[1-9]|[12][0-9]|3[01])[T ](0[0-9]|1[0-9]|2[0-3]):(0\d|[0-5]\d)(:(0\d|[0-5]\d)(\.\d{3})?(Z)?)?/) {
        return false
    } else {
        return true
    }
} else {
    return true
}
{% endblock %}



